{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "anhskohbo/no-captcha": "^3.3",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "brian2694/laravel-toastr": "^5.54",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.8",
        "haruncpi/laravel-id-generator": "^1.0",
        "intervention/image": "^2.5",
        "jomweb/billplz": "^4.1",
        "laravel/framework": "^8.12",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "nesbot/carbon": "^2.43",
        "nwidart/laravel-modules": "^8.2",
        "php-http/curl-client": "^2.2",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/message": "^1.11",
        "realrashid/sweet-alert": "^3.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^3.18",
        "uxweb/sweet-alert": "^2.0",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4.22"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Seeders\\": "database/seeders/"

        },
    "files": [
        "app/Http/Helpers/DateTime.php",
        "app/Http/Helpers/GetCart.php",
        "app/Http/Helpers/Helper.php",
        "app/Http/Helpers/Footer.php",
        "app/Http/Helpers/Sendnotification.php",
        "app/Http/Helpers/CreateNotification.php",
        "app/Http/Helpers/StatusChange.php"
    ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
